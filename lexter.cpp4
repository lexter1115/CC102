#include <iostream>
using namespace std;

void patternD(int rows) {
    for (int i = 1; i <= rows; i++) { // Loop for each row
        for (int j = i; j >= 1; j--) { // Print numbers in descending order
            cout << j << " ";
        }
        cout << endl;
    }
}

int main() {
    int rows = 8;
    cout << "Pattern (d):" << endl;
    patternD(rows);
    return 0;
}
